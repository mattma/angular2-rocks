{
  "name": "angular2-rocks",
  "version": "0.0.1",
  "description": "An Angular 2 Starter kit",
  "author": {
    "name": "Matt Ma",
    "email": "matt@mattmadesign.com",
    "url": "http://mattmadesign.com"
  },
  "homepage": "https://github.com/mattma/angular2-rocks",
  "license": "MIT",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf node_modules doc typings coverage dist npm-debug.log && npm cache clean",
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:prod",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "NODE_ENV=production ./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "server": "npm run server:dev",
    "server:dev": "./node_modules/.bin/webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/app/",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "./node_modules/.bin/http-server dist --cors",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "lint": "./node_modules/.bin/tslint 'src/app/**/*.ts'",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "./node_modules/.bin/protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "test": "NODE_ENV=test node --max-old-space-size=4096 ./node_modules/.bin/karma start",
    "ci": "npm run e2e && npm run test",
    "doc": "./node_modules/.bin/typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/app",
    "start": "npm run server",
    "start:hmr": "npm run server:dev:hmr",
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install",
    "preversion": "npm test",
    "version": "npm run build:dev",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@angular2-material/button": "2.0.0-alpha.4",
    "@angular2-material/card": "2.0.0-alpha.4",
    "@angular2-material/checkbox": "2.0.0-alpha.4",
    "@angular2-material/core": "2.0.0-alpha.4",
    "@angular2-material/input": "2.0.0-alpha.4",
    "@angular2-material/list": "2.0.0-alpha.4",
    "@angular2-material/progress-bar": "2.0.0-alpha.4",
    "@angular2-material/progress-circle": "2.0.0-alpha.4",
    "@angular2-material/radio": "2.0.0-alpha.4",
    "@angular2-material/sidenav": "2.0.0-alpha.4",
    "@angular2-material/toolbar": "2.0.0-alpha.4",
    "@angular/http": "2.0.0-rc.1",
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@ngrx/store": "^1.5.0",
    "core-js": "^2.2.2",
    "cuid": "^1.3.8",
    "immutable": "^3.7.6",
    "redux": "^3.3.1",
    "redux-localstorage": "^0.4.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  },
  "devDependencies": {
    "angular2-hmr": "~0.6.0",
    "awesome-typescript-loader": "~0.17.0",
    "codelyzer": "0.0.18",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^0.5.5",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.6.0",
    "parse5": "^2.1.5",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.8.2",
    "protractor": "^3.2.2",
    "raw-loader": "0.5.1",
    "redux-logger": "^2.3.2",
    "remap-istanbul": "^0.6.3",
    "rimraf": "^2.5.2",
    "rucksack-css": "^0.8.5",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "1.1.1",
    "ts-node": "^0.7.1",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.3.12",
    "typescript": "~1.8.9",
    "typings": "^0.8.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattma/angular2-rocks.git"
  },
  "bugs": {
    "url": "https://github.com/mattma/angular2-rocks/issues"
  },
  "engines": {
    "node": ">= 4.2.1 <= 5",
    "npm": ">= 2"
  }
}
