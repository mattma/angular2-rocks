{
  "name": "angular2-rocks",
  "version": "0.0.1",
  "description": "An Angular 2 Starter kit",
  "author": {
    "name": "Matt Ma",
    "email": "matt@mattmadesign.com",
    "url": "http://mattmadesign.com"
  },
  "homepage": "https://github.com/mattma/angular2-rocks",
  "license": "MIT",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf node_modules doc typings coverage dist npm-debug.log && npm cache clean",
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "./node_modules/.bin/webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "watch:prod": "./node_modules/.bin/webpack --watch --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "build": "npm run build:prod",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "NODE_ENV=production ./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "server": "npm run server:dev",
    "server:dev": "./node_modules/.bin/webpack-dev-server --progress --profile --colors --display-error-details --display-cached",
    "server:prod": "./node_modules/.bin/http-server dist --cors",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "lint": "./node_modules/.bin/tsconfig-lint",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "./node_modules/.bin/ts-node ./node_modules/.bin/protractor",
    "e2e:live": "./node_modules/.bin/ts-node ./node_modules/.bin/protractor --elementExplorer",
    "test": "./node_modules/.bin/karma start",
    "ci": "npm run e2e && npm run test",
    "doc": "./node_modules/.bin/typedoc --options typedoc.json --exclude **/*.spec.ts src/app",
    "start": "npm run server:dev",
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install",
    "preversion": "npm test",
    "version": "npm run build:dev",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.6",
    "cuid": "^1.3.8",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "es7-reflect-metadata": "^1.5.5",
    "immutable": "^3.7.6",
    "redux": "^3.2.1",
    "redux-localstorage": "^0.4.0",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.14"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^0.3.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "es6-promise-loader": "^1.0.1",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.8.1",
    "http-server": "^0.8.5",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs2-launcher": "^0.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.8.0",
    "protractor": "^3.0.0",
    "raw-loader": "0.5.1",
    "redux-logger": "^2.3.2",
    "reflect-metadata": "0.1.2",
    "remap-istanbul": "^0.5.1",
    "rimraf": "^2.5.1",
    "rucksack-css": "^0.8.5",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "ts-helper": "0.0.1",
    "ts-loader": "^0.8.1",
    "ts-node": "^0.5.5",
    "tsconfig-lint": "^0.5.0",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "1.7.5",
    "typings": "^0.6.7",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-load-plugins": "^0.1.2",
    "webpack-md5-hash": "^0.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattma/angular2-rocks.git"
  },
  "bugs": {
    "url": "https://github.com/mattma/angular2-rocks/issues"
  },
  "engines": {
    "node": ">= 4.2.1 <= 5",
    "npm": ">= 3"
  }
}
