{
  "name": "angular2-rocks",
  "version": "0.0.1",
  "description": "An Angular 2 Starter kit",
  "author": {
    "name": "Matt Ma",
    "email": "matt@mattmadesign.com",
    "url": "http://mattmadesign.com"
  },
  "homepage": "https://github.com/mattma/angular2-rocks",
  "license": "MIT",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf node_modules doc typings coverage dist npm-debug.log && npm cache clean && npm run tsd:clean",
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "watch": "npm run watch:dev",
    "watch:dev": "./node_modules/.bin/webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "watch:prod": "./node_modules/.bin/webpack --watch --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "server": "npm run server:dev",
    "server:dev": "./node_modules/.bin/webpack-dev-server --progress --profile --colors --display-error-details --display-cached",
    "server:prod": "./node_modules/.bin/http-server dist --cors",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "lint": "./node_modules/.bin/tsconfig-lint",
    "e2e": "./node_modules/.bin/protractor",
    "test": "./node_modules/.bin/karma start",
    "ci": "npm run e2e && npm run test",
    "doc": "./node_modules/.bin/typedoc --options typedoc.json src/**/*.ts",
    "start": "npm run server:dev",
    "tsd": "./node_modules/.bin/tsd install",
    "tsd:clean": "./node_modules/.bin/tsd purge",
    "postinstall": "npm run tsd && npm run webdriver:update"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "es6-shim": "^0.34.1",
    "es6-promise": "^3.0.2",
    "es7-reflect-metadata": "^1.4.0",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.0",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.7.0",
    "http-server": "^0.8.5",
    "imports-loader": "^0.6.4",
    "istanbul-instrumenter-loader": "^0.1.3",
    "json-loader": "^0.5.3",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "postcss-loader": "^0.8.0",
    "protractor": "^3.0.0",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.5.1",
    "rimraf": "^2.5.0",
    "rucksack-css": "^0.8.5",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.4.3",
    "tsd": "^0.6.5",
    "tslint": "^3.2.1",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.5",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0",
    "webpack-md5-hash": "0.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattma/angular2-rocks.git"
  },
  "bugs": {
    "url": "https://github.com/mattma/angular2-rocks/issues"
  },
  "engines": {
    "node": ">= 4.2.1 <= 5",
    "npm": ">= 3"
  }
}
